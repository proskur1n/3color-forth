graph {
  ratio = 1;

  1 -- 2;
  1 -- 3;
  2 -- 3;
  2 -- 5;
  3 -- 4;
  3 -- 5;
  4 -- 5;
  4 -- 6;
  #4 -- 7;
  5 -- 6;
  #5 -- 7;

  1 [fontsize=22, style=filled, fillcolor=tomato];
  2 [fontsize=22, style=filled, fillcolor=skyblue];
  3 [fontsize=22, style=bold, color=mediumseagreen];
  4 [fontsize=22];
  5 [fontsize=22];
  6 [fontsize=22];
}
